<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	
	<Operation Class="PatchOperationFindMod">
    <mods>
		<li>Combat Extended Guns without replacelib</li>
    </mods>
		<match Class="PatchOperationSequence">
		<operations>
		
		<li Class="PatchOperationFindMod">
		<mods>
			<li>Rimsenal - Augmented Vanilla Pack</li>
		</mods>
			<match Class="PatchOperationSequence">
			<operations>
			
			<li Class="PatchOperationAdd">
				<xpath>/Defs</xpath>
				<value>
				
					<!--
					<ThingStyleDef>
						<defName>Style_MilitiaRifleB</defName>
						<graphicData>
							<texPath>Things/A16/MilitiaRifle/MilitiaRifleB</texPath>
							<graphicClass>Graphic_Single</graphicClass>
						</graphicData>
					</ThingStyleDef>
					-->
					<ThingStyleDef>
						<defName>Style_RusticFNFALMM</defName>
						<graphicData>
							<texPath>Things/Weapons/FNFAL</texPath>
							<graphicClass>Graphic_Single</graphicClass>
							<drawSize>(1.25,1.25)</drawSize>
						</graphicData>
					</ThingStyleDef>
					
					<ThingStyleDef>
						<defName>MM_Rustic_RPD</defName>
						<graphicData>
							<texPath>Things/Weapons/RPD</texPath>
							<graphicClass>Graphic_Single</graphicClass>
							<drawSize>(1.2,1.2)</drawSize>
						</graphicData>
					</ThingStyleDef>
					
					<ThingStyleDef>
						<defName>MM_Rustic_PKM</defName>
						<graphicData>
							<texPath>Things/Weapons/PKM</texPath>
							<graphicClass>Graphic_Single</graphicClass>
							<drawSize>(1.3,1.3)</drawSize>
						</graphicData>
					</ThingStyleDef>	
				
				</value>
			</li>
			
			<li Class="PatchOperationReplace">
				<xpath>/Defs/ThingDef[defName="CE_Gun_FNFAL"]/graphicData</xpath>
				<value>
					<graphicData>
					  <texPath>Things/A16/BattleRifle</texPath>
					  <graphicClass>Graphic_Single</graphicClass>
					</graphicData>
				</value>
			</li>

			<li Class="PatchOperationAdd">
				<xpath>/Defs/StyleCategoryDef[defName="Techist"]/thingDefStyles</xpath>
				<value>
						<li>
							<thingDef>CE_Gun_FNFAL</thingDef>
							<styleDef>Techist_Gun_RSBattleRifle</styleDef>
						</li>
				</value>
			</li>
			
			<li Class="PatchOperationAdd">
				<xpath>/Defs/StyleCategoryDef[defName="Morbid"]/thingDefStyles</xpath>
				<value>
						<li>
							<thingDef>CE_Gun_FNFAL</thingDef>
							<styleDef>Morbid_Gun_RSBattleRifle</styleDef>
						</li>
				</value>
			</li>
			
			<li Class="PatchOperationAdd">
				<xpath>/Defs/StyleCategoryDef[defName="Rustic"]/thingDefStyles</xpath>
				<value>
						<li>
							<thingDef>CE_Gun_FNFAL</thingDef>
							<styleDef>Style_RusticFNFALMM</styleDef>
						</li>
				</value>
			</li>
			
			<li Class="PatchOperationReplace">
				<xpath>/Defs/ThingStyleDef[defName="Rustic_Gun_RSCarbine"]/graphicData</xpath>
				<value>
					<graphicData>
						<texPath>Things/Weapons/AKMS-U</texPath>
						<graphicClass>Graphic_Single</graphicClass>
						<drawSize>(1.0,1.0)</drawSize>
					</graphicData>
				</value>
			</li>
			
			<li Class="PatchOperationAdd">
				<xpath>/Defs/StyleCategoryDef[defName="Rustic"]/thingDefStyles</xpath>
				<value>
						<li>
							<thingDef>CE_Gun_MSixty</thingDef>
							<styleDef>MM_Rustic_PKM</styleDef>
						</li>
						<li>
							<thingDef>CE_Gun_MTwoFourNine</thingDef>
							<styleDef>MM_Rustic_RPD</styleDef>
						</li>
				</value>
			</li>
			
			<li Class="PatchOperationFindMod">
			<mods>
				<li>Rimsenal Style Pack - Sophian</li>
			</mods>
				<match Class="PatchOperationSequence">
				<operations>
				
				<li Class="PatchOperationAdd">
					<xpath>/Defs/StyleCategoryDef[defName="RSSophian"]/thingDefStyles</xpath>
					<value>
							<li>
								<thingDef>CE_Gun_ChargePistol</thingDef>
								<styleDef>Gun_ChargeSidearmSophian</styleDef>
							</li>
							<li>
								<thingDef>CE_Gun_ChargeShotgun</thingDef>
								<styleDef>Gun_ScatterChargerSophian</styleDef>
							</li>
					</value>
				</li>
				
				</operations>
				</match>
			</li>
			
			<li Class="PatchOperationFindMod">
			<mods>
				<li>Rimsenal Style Pack - Urbworld</li>
			</mods>
				<match Class="PatchOperationSequence">
				<operations>
				
				<li Class="PatchOperationAdd">
					<xpath>/Defs/StyleCategoryDef[defName="RSUrb"]/thingDefStyles</xpath>
					<value>
							<li>
								<thingDef>CE_Gun_ChargePistol</thingDef>
								<styleDef>Gun_ChargeSidearmUrb</styleDef>
							</li>
							<li>
								<thingDef>CE_Gun_ChargeShotgun</thingDef>
								<styleDef>Gun_ScatterChargerUrb</styleDef>
							</li>
							<li>
								<thingDef>CE_Gun_WinchesterNintyFour</thingDef>
								<styleDef>Gun_RepeaterUrb</styleDef>
							</li>
							<li>
								<thingDef>CE_Gun_AKM</thingDef>
								<styleDef>Gun_MilitiaRifleUrb</styleDef>
							</li>
							<li>
								<thingDef>CE_Gun_FNFAL</thingDef>
								<styleDef>Gun_RSBattleRifleUrb</styleDef>
							</li>
					</value>
				</li>
				
				</operations>
				</match>
			</li>
			
			<li Class="PatchOperationFindMod">
			<mods>
				<li>Rimsenal Style Pack - French</li>
			</mods>
				<match Class="PatchOperationSequence">
				<operations>
				
				<li Class="PatchOperationAdd">
					<xpath>/Defs/StyleCategoryDef[defName="RSFrench"]/thingDefStyles</xpath>
					<value>
							<li>
								<thingDef>CE_Gun_FNFAL</thingDef>
								<styleDef>Gun_RSBattleRifleFrench</styleDef>
							</li>
					</value>
				</li>
				
				</operations>
				</match>
			</li>
			
			<li Class="PatchOperationFindMod">
			<mods>
				<li>Rimsenal Style Pack - Murica</li>
			</mods>
				<match Class="PatchOperationSequence">
				<operations>
				
				<li Class="PatchOperationAdd">
					<xpath>/Defs/StyleCategoryDef[defName="RSMurica"]/thingDefStyles</xpath>
					<value>
							<li>
								<thingDef>CE_Gun_AKM</thingDef>
								<styleDef>Gun_MilitiaRifleMurica</styleDef>
							</li>
							<li>
								<thingDef>CE_Gun_FNFAL</thingDef>
								<styleDef>Gun_MilitiaRifleMurica</styleDef>
							</li>
					</value>
				</li>
				
				</operations>
				</match>
			</li>
	
			</operations>
			</match>
		</li>
	
		</operations>
		</match>
	</Operation>
	
</Patch>