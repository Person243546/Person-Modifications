<?xml version="1.0" encoding="utf-8" ?>
<Patch>
		
	<Operation Class="PatchOperationFindMod">
    <mods>
		<li>Rebound (Continued)</li>
    </mods>
		<match Class="PatchOperationSequence">
		<operations>
			
			<li Class="PatchOperationAdd">
				<xpath>/Defs/TraitDef[
				defName = "ProjectileInversion_Trait"
				]/degreeDatas/li</xpath>
				<value>
				<statOffsets>
					<Suppressability>-1</Suppressability>
				</statOffsets>
				</value>
			</li>
			
		</operations>
		</match>
	</Operation>
	
	
</Patch>